    echo -e "$OKRED                   (_ (   \  (     >    \)"
    echo -e "$OKRED                    \_( _ <         >_>'"
    echo -e "$OKRED                       ~ \`-i' ::>|--\""
    echo -e "$OKRED                           I;|.|.|"
